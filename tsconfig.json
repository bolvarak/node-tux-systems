{
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"lib": [
			"es2018"
		],
		"module": "commonjs",
		"outDir": "./Build",
		"removeComments": true,
		"sourceMap": true,
		"sourceRoot": "./Source",
		"strict": true,
		"target": "es2018"
	},
	"exclude": [
		"node_modules",
		"./Build"
	]
}

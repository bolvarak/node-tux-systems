{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"lib": [
			"es2017",
			"dom"
		],
		"checkJs": true,
		"declaration": true,
		"sourceMap": true,
		"outDir": "./dist/out-tsc",
		"removeComments": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"baseUrl": "./",
		"typeRoots": [
			"node_modules/@types"
		],
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}
